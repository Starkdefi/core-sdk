{
  "name": "@starkdefi/core-sdk",
  "author": {
    "name": "StarkDefi Labs",
    "url": "https://starkdefi.com"
  },
  "description": "SDK for building StarkDefi applications",
  "version": "2.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Starkdefi/core-sdk"
  },
  "keywords": [
    "StarkDefi",
    "Starknet"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "prepublishOnly": "tsdx build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/core-sdk.esm.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@starknet-react/chains": "^4.0.3",
    "starknet": "^7.6.4",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1",
    "whatwg-fetch": "^3.6.20"
  },
  "jest": {
    "verbose": false
  }
}
